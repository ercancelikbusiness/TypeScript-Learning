{
  "compilerOptions": {
    "target": "ES6",                               
    "module": "ESNext",                            
    "moduleResolution": "Node",                    
    "esModuleInterop": true,                       
    "forceConsistentCasingInFileNames": true,      
    "skipLibCheck": true,                          
    "strict": true,                                
    "isolatedModules": true                       
  }
}
